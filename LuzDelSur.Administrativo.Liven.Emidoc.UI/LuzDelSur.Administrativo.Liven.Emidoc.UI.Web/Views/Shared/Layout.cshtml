<!DOCTYPE html>
<html style="height: 100%;">
@RenderPage("~/Views/Home/header.cshtml")
<meta charset="utf-8" />
<meta http-equiv="X-UA-Compatible" content="Chrome=1">
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta name="description" content="Neon Admin Panel" />
<meta name="author" content="" />
<title></title>
<link rel="stylesheet" type="text/css" href="@Url.Content("~/Css/datepicker.css")" />
<link rel="stylesheet" type="text/css" href="@Url.Content("~/Css/bootstrap.css")" />
<script type="text/javascript" src="@Url.Content("~/Scripts/js/jquery-1.8.3.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/js/bootstrap.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/EjecutarFuncionalidad.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/js/bootstrap-datepicker.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/js/jquery.mask.min.js")"></script>
<style>
    .datepicker
    {
        z-index: 1151 !important;
    }
</style>
<body>
    <script type="text/javascript">
        var rutaProyecto = '@Request.ApplicationPath';
        if (rutaProyecto != "/") {
            rutaProyecto += "/";
        }
    </script>
    <div id="DivErrores">
    </div>
    <div class="row" id="DivContente">
        <div class="col-xs-12 col-sm-12 col-md-12">
            @RenderBody()
            @RenderSection("scripts", required: false)
        </div>
    </div>

     <div class="row" id="DivCargando" style="display:none;">
     <center>
      <img src="@Url.Content("~/iconos/Cargando.gif")"/> 
     </center>        
    </div>
   
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12">
            @RenderPage("~/Views/Home/footer.cshtml")
            </div>
        </div>
</body>
</html>
